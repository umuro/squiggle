    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "resolutions": {
    "@types/react": "17.0.43"
  },
  "source": "./src/index.ts",
  "main": "dist/bundle.js",
  "types": "dist/src/index.d.ts"
